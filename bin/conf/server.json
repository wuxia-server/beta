{
    "Settings":{
        "MongodbURL":      	    "mongodb://xxx" ,
        "MongodbDB":      	    "ludo-joy",
        "Nats":{
            "uri": "nats://127.0.0.1:4222",
            "user": "",
            "password": ""
        }
    },
    "Module":{
        "gate":[
            {
                //Id在整个Module中必须唯一,不能重复
                "Id":"Gate001",
                //这个模块所属进程,非常重要,进程会根据该参数来判断是否需要运行该模块 [development]为默认值代表开发环境
                "ProcessID":"development",
                "Settings":{
                    "WSAddr":      	 ":3653",
                    "TCPAddr":     	 ":3563",
                    "MaxMsgLen":     4096,
                    "HTTPTimeout":   10,
                    "MaxConnNum" :   20000,
                    "Tls"        :   false,
                    "CertFile"       :   "./bin/sslkey/h5link.com.pem",
                    "KeyFile"        :   "./bin/sslkey/h5link.com.key",
                    //Session持久化心跳包 单位/秒
                    "MinHBStorage"  :   60,
                    "RedisUrl":  "redis://:payplug@115.28.62.110:6379/11"
                }
            }
        ],
        "account":[
            {
                "Id":"Account001",
                "ProcessID":"development"
            }
        ],
        "webapp":[
            {
                "Id":"WebApp001",
                "ProcessID":"development",
                "Settings":{
                    "StaticPath":      	 "/opt/go/loolgame/static",
                    "ResourcePath":       "bin/devildom",
                    "Port": 7370
                }
            }
        ],
        "httpgateway":[
            {
                "Id":"httpgateway",
                "ProcessID":"development",
                "Settings":{
                    "StaticPath":      	 "/opt/go/loolgame/static",
                    "ResourcePath":       "bin/devildom",
                    "Port": 7375
                }
            }
        ]
    },
    "Log": {
        "contenttype":"text/plain",
        "multifile": {
            "maxLines": 0,
            "maxsize": 0,
            "daily": true,
            "rotate": true,
            "perm": "0600",
            "separate": [
                "error"
            ]
        }
    },
    "BI": {
        "file": {
            "maxLines": 0,
            "maxsize": 0,
            "daily": true,
            "rotate": true,
            "perm": "0600",
            "prefix": "bi."
        }
    },
    "Mqtt":{
        // 最大写入包队列缓存
        "WirteLoopChanNum": 10,
        // 最大读取包队列缓存
        "ReadPackLoop": 1,
        // 读取超时
        "ReadTimeout": 600,
        // 写入超时
        "WriteTimeout": 300
    },
    "Rpc":{
        "MaxCoroutine":100,
        // 远程访问最后期限值 单位秒 这个值指定了在客户端可以等待服务端多长时间来应答
        "RpcExpired": 3,
        //默认是 false 不打印
        "LogSuccess":true
    }
}